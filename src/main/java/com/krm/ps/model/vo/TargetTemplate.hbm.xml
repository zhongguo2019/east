<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.krm.ps.model.vo.TargetTemplate" table="collect_target">

		<id name="pkid" column="pkid" type="java.lang.Long">
			<generator class="native">
				<!-- <param name="sequence">COLLECT_TARGET_seq</param> -->
				<param name="increment">pkid</param>
				<!-- To add non XDoclet generator parameters, create a file named hibernate-generator-params-ReportTarget.xml 
					containing the additional parameters and place it in your merge dir. -->
			</generator>
		</id>
		
		<property name="reportId" type="java.lang.Long" update="true"
			insert="true" column="report_id" />
		
		<property name="targetName" type="java.lang.String" update="true"
			insert="true" column="target_name" />
			
		<property name="targetField" type="java.lang.String" update="true"
			insert="true" column="target_field" />
			
		<property name="targetOrder" type="java.lang.Integer" update="true"
			insert="true" column="target_order" />	
			
		<property name="beginDate" type="java.lang.String" update="true"
			insert="true" column="begin_date" />		
			
		<property name="endDate" type="java.lang.String" update="true"
			insert="true" column="end_date" />
			
		<property name="createDate" type="java.lang.String" update="true"
			insert="true" column="create_date" />	
				
		<property name="editDate" type="java.lang.String" update="true"
			insert="true" column="edit_date" />	
			
		<property name="status" type="java.lang.Integer" update="true"
			insert="true" column="status" />	
			
		<property name="dicPid" type="java.lang.String" update="true"
			insert="true" column="dic_pid" />	
		
		<property name="phytable" type="java.lang.String" update="true"
			insert="true" column="phy_table" />	
				
		<property name="phytableid" type="java.lang.Long" update="true"
			insert="true" column="phy_id" />
		
		<!-- To add non XDoclet property mappings, create a file named hibernate-properties-ReportTarget.xml 
			containing the additional properties and place it in your merge dir. -->

	</class>

</hibernate-mapping>
