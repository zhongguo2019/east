<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.krm.ps.model.vo.ReportFile"
        table="rep_file"
    >

        <id
            name="pkId"
            column="pkid"
            type="java.lang.Long"
        >
            <generator class="native">
                <param name="sequence">rep_file_pkid_seq</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-ReportFile.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="dataDate"
            type="java.lang.String"
            update="true"
            insert="true"
            column="data_date"
        />

        <property
            name="organId"
            type="java.lang.String"
            update="true"
            insert="true"
            column="organ_id"
        />

        <property
            name="createDate"
            type="java.lang.String"
            update="true"
            insert="true"
            column="create_date"
        />

        <property
            name="reportId"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="report_id"
        />

        <property
            name="status"
            type="java.lang.String"
            update="true"
            insert="true"
            column="status"
        />

        <property
            name="formatId"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="format_id"
        />

        <property
            name="reportData"
            type="java.lang.String"
            update="true"
            insert="true"
            column="report_data"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-ReportFile.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
