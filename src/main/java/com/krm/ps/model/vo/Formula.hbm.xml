<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.krm.ps.model.vo.Formula"
        table="code_formula"
    >

        <id
            name="pkId"
            column="pkid"
            type="java.lang.Long"
        >
            <generator class="native">
                <param name="sequence">code_formula_pkid_seq</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Formula.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="beginDate"
            type="java.lang.String"
            update="true"
            insert="true"
            column="begin_date"
        />

        <property
            name="createDate"
            type="java.lang.String"
            update="true"
            insert="true"
            column="create_date"
        />

        <property
            name="editDate"
            type="java.lang.String"
            update="true"
            insert="true"
            column="edit_date"
        />

        <property
            name="endDate"
            type="java.lang.String"
            update="true"
            insert="true"
            column="end_date"
        />

        <property
            name="itemId"
            type="java.lang.String"
            update="true"
            insert="true"
            column="item_id"
        />

        <property
            name="formulaName"
            type="java.lang.String"
            update="true"
            insert="true"
            column="name"
        />

        <property
            name="formulaNum"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="formula_num"
        />

        <property
            name="reportId"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="report_id"
        />

        <property
            name="explain"
            type="org.springframework.orm.hibernate3.support.ClobStringType"
            update="true"
            insert="true"
            column="explain"
        />

        <property
            name="formula"
            type="java.lang.String"
            update="true"
            insert="true"
            column="formula"
        />

        <property
            name="formulaType"
            type="java.lang.String"
            update="true"
            insert="true"
            column="formula_type"
        />

        <property
            name="sectId"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="sect_id"
        />

        <property
            name="fieldName"
            type="java.lang.String"
            update="true"
            insert="true"
            column="field_name"
        />

        <property
            name="frequency"
            type="java.lang.String"
            update="true"
            insert="true"
            column="frequency"
        />

        <property
            name="sign"
            type="java.lang.String"
            update="true"
            insert="true"
            column="sign"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Formula.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
