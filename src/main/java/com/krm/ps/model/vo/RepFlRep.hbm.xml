<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.krm.ps.model.vo.RepFlRep"
        table="DSP_FL_FOMAT_REP"
    >

        <id
            name="pkid"
            column="pkid"
            type="java.lang.Long"
        >
            <generator class="native">
                <param name="sequence">DSP_FL_FLATTBUTTE_SEQ</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Dictionary.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="repflid"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="repflid"
        />

        <property
            name="repid"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="repid"
        />

        <property
            name="replable"
            type="java.lang.String"
            update="true"
            insert="true"
            column="replable"
        />

       
        <property
            name="hvattr"
            type="java.lang.String"
            update="true"
            insert="true"
            column="hvattr"
        />
        <property
            name="status"
            type="java.lang.String"
            update="true"
            insert="true"
            column="status"
        />
        <property
            name="hvfiled"
            type="java.lang.String"
            update="true"
            insert="true"
            column="hvfiled"
        />
       <property
            name="repdesc"
            type="java.lang.String"
            update="true"
            insert="true"
            column="repdesc"
        />
       <property
            name="reporder"
            type="java.lang.String"
            update="true"
            insert="true"
            column="reporder"
        />
       <property
            name="looplab"
            type="java.lang.String"
            update="true"
            insert="true"
            column="LOOPLAB"
        />
       <property
            name="tablelab"
            type="java.lang.String"
            update="true"
            insert="true"
            column="TABLELAB"
        />
       <property
            name="wherecondition"
            type="java.lang.String"
            update="true"
            insert="true"
            column="WHERECONDITION"
        />
     

    </class>

</hibernate-mapping>
