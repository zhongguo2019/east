                <param name="sequence">CODE_REP_CONFIGURE_SEQ</param>
            </generator>
        </id>

      <property name="organ_id" type="java.lang.String" update="true" insert="true" column="organ_id" />

	  <property name="organ_name" type="java.lang.String" update="true" insert="true" column="organ_name" />
	  
