	  <property name="report_name" type="java.lang.String" update="true" insert="true" column="report_name" />
	  
	  
	  <property name="target_id" type="java.lang.String" update="true" insert="true" column="target_id" />

	  <property name="target_name" type="java.lang.String" update="true" insert="true" column="target_name" />
	  
	  
	  <property name="rtype" type="java.lang.String" update="true" insert="true" column="rtype" />

	  <property name="threshold" type="java.lang.String" update="true" insert="true" column="threshold" />
	  
	  <property name="attrbuter1" type="java.lang.String" update="true" insert="true" column="attrbuter1" />

	  <property name="attrbuter2" type="java.lang.String" update="true" insert="true" column="attrbuter2" />
	  
	   <property name="attrbuter3" type="java.lang.String" update="true" insert="true" column="attrbuter3" />
				
		

    </class>

</hibernate-mapping>
