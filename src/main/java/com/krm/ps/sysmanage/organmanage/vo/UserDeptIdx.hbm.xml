<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.krm.ps.sysmanage.organmanage.vo.UserDeptIdx"
        table="code_user_dept_idx"
    >

        <id
            name="pkid"
            column="pkid"
            type="java.lang.Long"
        >
            <generator class="native">
                <param name="sequence">code_user_dept_idx_seq</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-UserDeptIdx.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="deptId"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="dept_id"
        />

        <property
            name="deptName"
            type="java.lang.String"
            update="true"
            insert="true"
            column="dept_name"
        />

        <property
            name="organCode"
            type="java.lang.String"
            update="true"
            insert="true"
            column="organ_code"
        />

        <property
            name="organName"
            type="java.lang.String"
            update="true"
            insert="true"
            column="organ_name"
        />

        <property
            name="userId"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="user_id"
        />

        <property
            name="userName"
            type="java.lang.String"
            update="true"
            insert="true"
            column="user_name"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-UserDeptIdx.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
