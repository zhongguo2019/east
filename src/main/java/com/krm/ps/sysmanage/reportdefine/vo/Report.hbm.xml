<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.krm.ps.sysmanage.reportdefine.vo.Report"
        table="code_rep_report"
    >

        <id
            name="pkid"
            column="pkid"
            type="java.lang.Long"
        >
            <generator class="native">
                <param name="sequence">code_rep_report_seq</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Report.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="conCode"
            type="java.lang.String"
            update="true"
            insert="true"
            column="con_code"
        />

        <property
            name="beginDate"
            type="java.lang.String"
            update="true"
            insert="true"
            column="begin_date"
        />

        <property
            name="code"
            type="java.lang.String"
            update="true"
            insert="true"
            column="code"
        />

        <property
            name="description"
            type="java.lang.String"
            update="true"
            insert="true"
            column="description"
        />

        <property
            name="endDate"
            type="java.lang.String"
            update="true"
            insert="true"
            column="end_date"
        />

        <property
            name="frequency"
            type="java.lang.String"
            update="true"
            insert="true"
            column="frequency"
        />

        <property
            name="moneyunit"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="moneyunit"
        />
         <property
            name="isIncrement"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="IS_INCREMENT"
        />

        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            column="name"
        />

        <property
            name="phyTable"
            type="java.lang.String"
            update="true"
            insert="true"
            column="phy_table"
        />

        <property
            name="reportType"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="report_type"
        />

        <property
            name="rol"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="rol"
        />

        <property
            name="status"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="status"
        />

        <property
            name="isSum"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="is_sum"
        />

        <property
            name="showOrder"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="show_order"
        />

        <property
            name="precision"
            type="java.lang.String"
            update="true"
            insert="true"
            column="precision"
        />

        <property
            name="createId"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="createid"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Report.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
